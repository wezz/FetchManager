{
  "name": "@wezz/fetchmanager",
  "version": "1.0.12",
  "description": "A script that improves a javascript fetch requests with request throttling, managing querystring parameters and delayed request start",
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "vite",
    "build": "tsc && vite build",
    "prepare": "npm run build",
    "pack": "npm run build",
    "trypublish": "npm publish --access public || true",
    "eslint": "eslint --ext .ts",
    "prettier": "prettier --write \"**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wezz/FetchManager"
  },
  "author": "Wezz Balk <github@wezz.se> (https://github.com/wezz)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wezz/FetchManager/issues"
  },
  "homepage": "https://github.com/wezz/FetchManager",
  "keywords": [
    "fetch",
    "json",
    "request"
  ],
  "devDependencies": {
    "@eslint/js": "~9.25.1",
    "@types/node": "~22.14.1",
    "eslint": "~9.25.1",
    "prettier": "~3.5.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "~8.31.0",
    "vite": "~6.3.2",
    "vite-plugin-dts": "~4.5.3"
  },
  "dependencies": {
    "@wezz/store-manager": "0.1.4",
    "@wezz/window-reference-store": "0.1.3"
  }
}
